{
  "name": "nuxt-sitemap-example",
  "version": "1.0.0",
  "description": "nuxt sitemap example",
  "scripts": {
    "dev": "nodemon --exec babel-node -w server.js -w nuxt.config.js -w xml/ server.js",
    "build": "nuxt build && babel server.js --out-file server.prod.js",
    "start": "cross-env NODE_ENV=production node server.prod.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "dependencies": {
    "cross-env": "latest",
    "express": "latest",
    "nuxt": "latest",
    "xml": "latest"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "nodemon": "latest"
  }
}
